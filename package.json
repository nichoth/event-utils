{
  "name": "@nichoth/event-utils",
  "version": "0.0.0",
  "description": "Event namespacing stuff",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "preversion": "npm run lint && npm run deps && npm test",
    "postversion": "git push && git push --tags",
    "lint": "eslint .",
    "deps": "dependency-check package.json && dependency-check package.json --unused --entry test/*.js -i dependency-check -i eslint -i eslint-config-minlint -i tap-spec",
    "test": "tape test/*.js | tap-spec"
  },
  "dependencies": {
    "inherits": "^2.0.3",
    "nanobus": "^4.2.0"
  },
  "devDependencies": {
    "dependency-check": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-minlint": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/event-utils.git"
  },
  "author": "nichoth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/event-utils/issues"
  },
  "homepage": "https://github.com/nichoth/event-utils#readme"
}
